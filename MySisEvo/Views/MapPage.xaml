            <StackPanel>
                <telerik:RadDocking Name="RadDockAna" Margin="0,-5,0,0" MaxHeight="2048" >
            <telerik:RadDocking.DocumentHost>
                <telerik:RadSplitContainer>
                    <telerik:RadPaneGroup>
                        <telerik:RadPane Name="PaneMap" Header="Harita" >
                                    <telerik:RadMap x:Name="RadMap1"  ZoomLevel="7" MinZoomLevel="5" MaxZoomLevel="18" MouseEnter="PaneMap_MouseEnter" MouseLeave="PaneMap_MouseLeave" >
                                            <telerik:InformationLayer Name="layerDetayIn">
                                                <telerik:InformationLayer.ItemTemplate>
                                                    <DataTemplate >
                                                        <Canvas Width="40" Height="20">

                                                            <telerik:MapLayer.HotSpot>
                                                                <telerik:HotSpot X="0.5" Y="1.0" ElementName="Pin" />
                                                            </telerik:MapLayer.HotSpot>

                                                            <Canvas x:Name="Pin" Margin="-10,-28,0,0" Height="32" Width="20" HorizontalAlignment="Center">
                                                                <Image Height="32" Width="20" Stretch="Fill" Source="/MySisEvo;component/Images/bullet_pin_green.png" />
                                                                <Canvas Height="50" Margin="-20,-30,0,0" Width="60">
                                                                    <Image Margin="10,25,0,0" Stretch="Fill" Cursor="Hand" Source="{Binding Path=AracIcon}" />
                                                                </Canvas>
                                                            </Canvas>
                                                            <Grid DataContext="{Binding Source={StaticResource EntityObject}}" 
                                                                  Margin="-200,-210,0,0" Background="Transparent">
                                                                <Canvas>

                                                                    <Rectangle Fill="White" Opacity="0.5" 
                                                                           Width="400" Height="200"
                                                                           RadiusX="10" RadiusY="10">
                                                                    </Rectangle>


                                                                    <!-- main rect -->
                                                                    <Rectangle Width="400" Height="200"  RadiusX="10" RadiusY="10"
                                                                           Opacity="0.5">
                                                                        <Rectangle.Fill>
                                                                            <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                                                                <GradientStop Offset="0" Color="DarkOliveGreen"/>
                                                                                <GradientStop Offset="1" Color="DarkOliveGreen"/>
                                                                            </LinearGradientBrush>
                                                                        </Rectangle.Fill>
                                                                    </Rectangle>

                                                                    <!-- transparent rect -->
                                                                    <Rectangle Width="398" Height="198" RadiusX="10" RadiusY="10">
                                                                        <Rectangle.Fill>
                                                                            <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                                                                                <GradientStop Offset="0" Color="DarkOliveGreen"/>
                                                                                <GradientStop Offset="1" Color="#00000000"/>
                                                                            </LinearGradientBrush>
                                                                        </Rectangle.Fill>
                                                                    </Rectangle>
                                                                    <telerik:RadGauge x:Name="radGauge"
                                                                          Margin="300,0,0,0" 
                                                                          Height="200" 
                                                                          Width="200">
                                                                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                                                            <telerik:RadialGauge>
                                                                                <telerik:RadialScale x:Name="radialScale" Min="0" Max="220" MajorTicks="10"
							                                                            MiddleTicks="1">

                                                                                    <telerik:RadialScale.MajorTick>
                                                                                        <telerik:MajorTickProperties />
                                                                                    </telerik:RadialScale.MajorTick>
                                                                                    <telerik:RadialScale.MiddleTick>
                                                                                        <telerik:MiddleTickProperties Length="0.07" />
                                                                                    </telerik:RadialScale.MiddleTick>
                                                                                    <telerik:RadialScale.MinorTick>
                                                                                        <telerik:MinorTickProperties Length="0.05" />
                                                                                    </telerik:RadialScale.MinorTick>

                                                                                    <telerik:RadialScale.Label>
                                                                                        <telerik:LabelProperties FontSize="10" />
                                                                                    </telerik:RadialScale.Label>

                                                                                    <telerik:IndicatorList>
                                                                                        <telerik:RadialBar x:Name="gauge1_radialBar" IsAnimated="True" Value="{Binding Path=Data.DT_H1Z}" />

                                                                                        <telerik:Needle x:Name="gauge1_needle" IsAnimated="true" Value="{Binding Path=Data.DT_H1Z}" />
                                                                                    </telerik:IndicatorList>
                                                                                </telerik:RadialScale>
                                                                            </telerik:RadialGauge>
                                                                            <Grid>
                                                                                <Grid.RowDefinitions>
                                                                                    <RowDefinition Height="0.60*" />
                                                                                    <RowDefinition Height="0.40*" />
                                                                                </Grid.RowDefinitions>

                                                                                <TextBlock Grid.Row="1" VerticalAlignment="Top" HorizontalAlignment="Center"
							                                                    Foreground="GhostWhite" FontFamily="CourierNew">HIZ</TextBlock>
                                                                            </Grid>
                                                                        </Grid>
                                                                    </telerik:RadGauge>
                                                                <StackPanel Orientation="Horizontal">
                                                                    <Border BorderThickness="2" CornerRadius="5" Margin="5" BorderBrush="White">
                                                                    <StackPanel Orientation="Vertical" Margin="5" >
                                                                        <StackPanel Orientation="Horizontal">
                                                                            <TextBlock Text="KONTAK"  Foreground="White" Width="50"/>
                                                                            <TextBlock Text=" = "  Foreground="White"/>
                                                                            <TextBlock Text="{Binding Path=Data.DT_KONTAK}" Foreground="White" />
                                                                        </StackPanel>
                                                                        <StackPanel Orientation="Horizontal" >
                                                                            <TextBlock Text="ENLEM"  Foreground="White" Width="50"/>
                                                                            <TextBlock Text=" = "  Foreground="White"/>
                                                                            <TextBlock Text="{Binding Path=Data.DT_ENLEM}" Foreground="White" />
                                                                        </StackPanel>
                                                                        <StackPanel Orientation="Horizontal" >
                                                                            <TextBlock Text="BOYLAM" Foreground="White" Width="50" />
                                                                            <TextBlock Text=" = "  Foreground="White"/>
                                                                            <TextBlock Text="{Binding Path=Data.DT_BOYLAM}" Foreground="White" />
                                                                        </StackPanel>
                                                                        <StackPanel Orientation="Horizontal" >
                                                                            <TextBlock Text="HIZ" Foreground="White" Width="50" />
                                                                            <TextBlock Text=" = "  Foreground="White"/>
                                                                            <TextBlock Text="{Binding Path=Data.DT_H1Z}" Foreground="White" />
                                                                            <TextBlock Text=" KM/H"  Foreground="White"/>
                                                                        </StackPanel>
                                                                        <StackPanel Orientation="Horizontal">
                                                                            <TextBlock Text="SERİ NO" Foreground="White" Width="50" />
                                                                            <TextBlock Text=" = "  Foreground="White"/>
                                                                            <TextBlock Text="{Binding Path=Arac.ARC_SERINO}" Foreground="White" />
                                                                        </StackPanel>
                                                                        <StackPanel Orientation="Horizontal">
                                                                            <TextBlock Text="PLAKA"  Foreground="White" Width="50"/>
                                                                            <TextBlock Text=" = "  Foreground="White"/>
                                                                            <TextBlock Text="{Binding Path=Arac.ARC_PLAKA}" Foreground="White" />
                                                                        </StackPanel>
                                                                        <StackPanel Orientation="Horizontal">
                                                                            <TextBlock Text="TEL"  Foreground="White" Width="50"/>
                                                                            <TextBlock Text=" = "  Foreground="White"/>
                                                                            <TextBlock Text="{Binding Path=Arac.ARC_TEL}" Foreground="White" />
                                                                        </StackPanel>
                                                                        <StackPanel Orientation="Horizontal">
                                                                            <TextBlock Text="MARKA" Foreground="White" Width="50" />
                                                                            <TextBlock Text=" = "  Foreground="White"/>
                                                                            <TextBlock Text="{Binding Path=Arac.ARC_MARKA}" Foreground="White" />
                                                                        </StackPanel>
                                                                        <StackPanel Orientation="Horizontal">
                                                                            <TextBlock Text="MODEL" Foreground="White" Width="50" />
                                                                            <TextBlock Text=" = "  Foreground="White"/>
                                                                            <TextBlock Text="{Binding Path=Arac.ARC_MODEL}" Foreground="White" />
                                                                        </StackPanel>
                                                                        <StackPanel Orientation="Horizontal">
                                                                            <TextBlock Text="RENK"  Foreground="White" Width="50"/>
                                                                            <TextBlock Text=" = "  Foreground="White"/>
                                                                            <TextBlock Text="{Binding Path=Arac.ARC_RENK}" Foreground="White" />
                                                                        </StackPanel>
                                                                        <StackPanel Orientation="Horizontal">
                                                                            <TextBlock Text="SAHIBI"  Foreground="White" Width="50"/>
                                                                            <TextBlock Text=" = "  Foreground="White"/>
                                                                            <TextBlock Text="{Binding Path=Arac.ARC_SAHIBI}" Foreground="White"/>
                                                                        </StackPanel>
                                                                    </StackPanel>
                                                                </Border>
                                                                    <Border BorderThickness="2" CornerRadius="5" Margin="5" BorderBrush="White">
                                                                        <StackPanel Orientation="Vertical">
                                                                            <Image Width="120" Stretch="Uniform" Margin="5" Source="{Binding Path=Arac.ARC_RES1M}">
                                                                                <Image.Effect>
                                                                                    <DropShadowEffect Color="Black" Direction="315" Opacity="0.8" ShadowDepth="7"/>
                                                                                </Image.Effect>
                                                                            </Image>
                                                                            <telerik:RadButton Margin="2" Height="35" Width="90" Click="btnGecmisKisa_Click">
                                                                                <telerik:RadButton.Content>
                                                                                    <StackPanel Orientation="Horizontal">
                                                                                        <Image Source="/MySisEvo;component/Images/arrow_undo.png" Height="30" Width="30" />
                                                                                        <TextBlock Text="iZLE" Foreground="Navy" Margin="5,7,0,0" />
                                                                                    </StackPanel>
                                                                                </telerik:RadButton.Content>
                                                                            </telerik:RadButton>
                                                                        </StackPanel>
                                                                    </Border>
                                                                </StackPanel>
                                                            </Canvas>

                                                            </Grid>


                                                        </Canvas>
                                                    </DataTemplate>
                                                </telerik:InformationLayer.ItemTemplate>
                                            </telerik:InformationLayer>

                                            <telerik:InformationLayer Name="layerDetayOut">
                                            <telerik:InformationLayer.ItemTemplate>
                                                <DataTemplate >
                                                    <Canvas>

                                                        <telerik:MapLayer.HotSpot>
                                                            <telerik:HotSpot X="0.5" Y="1.0" ElementName="Pin" />
                                                        </telerik:MapLayer.HotSpot>


                                                        <Canvas x:Name="Pin" Margin="-10,-28,0,0" Height="32" Width="20" HorizontalAlignment="Center">
                                                            <Image Height="32" Width="20" Stretch="Fill" Source="/MySisEvo;component/Images/bullet_pin_red.png" />
                                                            <Canvas Height="50" Margin="-20,-30,0,0" Width="60">
                                                                <Image Margin="10,25,0,0" Stretch="Fill" Cursor="Hand" Source="{Binding Path=AracIcon}" />
                                                            </Canvas>
                                                            
                                                        </Canvas>

                                                    </Canvas>
                                                </DataTemplate>
                                            </telerik:InformationLayer.ItemTemplate>

                                        </telerik:InformationLayer>


                                        </telerik:RadMap>
                        </telerik:RadPane>
                        <telerik:RadPane Name="PaneGeo" Header="GEO" Visibility="Collapsed">
                            <Grid HorizontalAlignment="Center">
                                <!--<telerik:RadMap x:Name="RadGeoMap"  ZoomLevel="7" MinZoomLevel="5" MaxZoomLevel="18">
                                </telerik:RadMap>-->
                            </Grid>
                        </telerik:RadPane>
                        <telerik:RadPane Name="PaneSonKonum" Header="Son Konumlar" Visibility="Collapsed">
                            <Grid HorizontalAlignment="Center">
                                <telerik:RadGridView Name="sonKonumGrid" AutoGenerateColumns="False"
                                    CanUserFreezeColumns="False"
                                    RowIndicatorVisibility="Collapsed" Foreground="Navy"
                                    ShowGroupPanel="False" ItemsSource="{Binding Path=AracDataList}">
                                        <telerik:RadGridView.Columns>
                                        <telerik:GridViewDataColumn Header="DURUM" ShowDistinctFilters="False">
                                            <telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <!--<TextBlock Margin="2" Text="{Binding Path=Arac.ARC_SERINO}" Foreground="Navy" />-->
                                                    <Grid>
                                                        <Image Grid.Column="1" Source="/MySisEvo;component/Images/Loff.png"  Height="25" Width="25" >
                                                            <ToolTipService.ToolTip>
                                                                <ToolTip>
                                                                    <TextBlock Text="Baglantı Yok"/>
                                                                </ToolTip>
                                                            </ToolTipService.ToolTip>
                                                        </Image>
                                                        <Image Visibility="{Binding Path=Arac.ARC_KOPMA}" Grid.Column="1" Source="/MySisEvo;component/Images/Lon.png"  Height="25" Width="25" >
                                                            <ToolTipService.ToolTip>
                                                                <ToolTip>
                                                                    <TextBlock Text="Baglantı Var"/>
                                                                </ToolTip>
                                                            </ToolTipService.ToolTip>
                                                        </Image>
                                                    </Grid>
                                                </DataTemplate>
                                            </telerik:GridViewDataColumn.CellTemplate>
                                        </telerik:GridViewDataColumn>
                                        <telerik:GridViewDataColumn Header="PLAKA" ShowDistinctFilters="False">
                                            <telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Margin="2" Text="{Binding Arac.ARC_PLAKA}" Foreground="Navy" />
                                                </DataTemplate>
                                            </telerik:GridViewDataColumn.CellTemplate>
                                        </telerik:GridViewDataColumn>
                                        <telerik:GridViewDataColumn Header="MARKA" ShowDistinctFilters="False">
                                            <telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Margin="2" Text="{Binding Arac.ARC_MARKA}" Foreground="Navy" />
                                                </DataTemplate>
                                            </telerik:GridViewDataColumn.CellTemplate>
                                        </telerik:GridViewDataColumn>
                                        <telerik:GridViewDataColumn Header="MODEL" ShowDistinctFilters="False">
                                            <telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Margin="2" Text="{Binding Arac.ARC_MODEL}" Foreground="Navy" />
                                                </DataTemplate>
                                            </telerik:GridViewDataColumn.CellTemplate>
                                        </telerik:GridViewDataColumn>
                                        <telerik:GridViewDataColumn Header="ENLEM" ShowDistinctFilters="False">
                                            <telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Margin="2" Text="{Binding Data.DT_ENLEM}" Foreground="Navy" />
                                                </DataTemplate>
                                            </telerik:GridViewDataColumn.CellTemplate>
                                        </telerik:GridViewDataColumn>
                                        <telerik:GridViewDataColumn Header="BOYLAM" ShowDistinctFilters="False">
                                            <telerik:GridViewDataColumn.CellTemplate>
                                                <DataTemplate>
                                                    <TextBlock Margin="2" Text="{Binding Data.DT_BOYLAM}" Foreground="Navy" />
